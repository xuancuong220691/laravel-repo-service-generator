{
  "name": "cuongnx/laravel-repo-service-generator",
  "description": "Generate repository-service structure for Laravel with interface and binding",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Cuong Nguyen",
      "email": "xuancuong220691@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "CuongNX\\RepoServiceGenerator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CuongNX\\RepoServiceGenerator\\Providers\\GeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
